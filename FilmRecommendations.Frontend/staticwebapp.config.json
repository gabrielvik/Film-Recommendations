{
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif,svg}", "/css/*", "/assets/*",  "/*.js", "/favicon.ico", "/robots.txt"]
  },
  "mimeTypes": {
    ".css": "text/css",
    ".js": "application/javascript"
  }
}